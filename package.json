{
  "name": "@moti-technologies/fastify-autoroutes",
  "version": "5.0.2",
  "description": "Map directory structure to routes",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs": "rm -rf ./docs/_sidebar.md && docsify generate ./docs",
    "docs:watch": "docsify serve ./docs",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MOTI-Technologies/fastify-autoroutes.git"
  },
  "keywords": [
    "fastify",
    "fastify-plugin",
    "fastify-autoroutes",
    "autoroute",
    "autorouting",
    "autoroutes"
  ],
  "author": "Giovanni Cardamone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MOTI-Technologies/fastify-autoroutes/issues"
  },
  "homepage": "https://github.com/MOTI-Technologies/fastify-autoroutes#readme",
  "devDependencies": {
    "@giovannicardamone/prettier-config": "^0.1.3",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.15",
    "coveralls": "^3.1.1",
    "docsify": "^4.13.1",
    "docsify-cli": "^4.4.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "fastify": "^5.6.2",
    "husky": "^9.1.7",
    "light-my-request": "^6.6.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@fastify/error": "^4.2.0",
    "fastify-plugin": "^5.1.0",
    "glob-promise": "^6.0.7"
  },
  "peerDependencies": {
    "fastify": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "contributors": [
    {
      "name": "Giovanni Cardamone",
      "email": "g.cardamone2@gmail.com",
      "url": "https://giovannicardamone.github.io"
    },
    {
      "name": "Gennaro Bosone",
      "email": "gennaro.bs@gmail.com",
      "url": "https://www.desidus.it"
    }
  ]
}
